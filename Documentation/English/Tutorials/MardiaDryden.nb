Cell[CellGroupData[{

Cell["\<\
We need a new timescale dtau to allow for the way that shape changes faster \
when size is small. We introduce a new innovation Brownian motion W in this \
new timescale.\
\>", "Text",
 Evaluatable->False,
 CellChangeTimes->{{3.923661961778594*^9, 3.9236619617933702`*^9}},
 CellID->1419865475,ExpressionUUID->"d46901bf-4672-49dd-bddd-e69319601a6f"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"dtau", "=", 
  FractionBox["dt", 
   SuperscriptBox["R", "2"]]}], "\n", 
 RowBox[{
  RowBox[{"Introduce", "[", 
   RowBox[{"W", ",", "dW"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"AddQuadVar", "[", 
   RowBox[{
    SuperscriptBox["dW", "2"], ",", "dtau"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"AddQuadVar", "[", 
   RowBox[{
    RowBox[{"dW", " ", "dR"}], ",", "0"}], "]"}], ";"}], "\n", 
 RowBox[{"AddDrift", "[", 
  RowBox[{"dW", ",", "0"}], "]"}], "\n", 
 RowBox[{"ItoStatus", "[", "]"}]}], "Input",
