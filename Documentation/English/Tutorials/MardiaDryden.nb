   RowBox[{"S", ",", 
    RowBox[{"dS", "==", 
     RowBox[{"dW", "+", 
      RowBox[{
       RowBox[{"Cot", "[", 
        RowBox[{"2", " ", "S"}], "]"}], " ", "dtau"}]}]}], ",", "S0"}], "]"}],
   ";"}], "\n", 
 RowBox[{"ItoStatus", "[", "]"}]}], "Input",
 PageWidth->DirectedInfinity[1],
 CellLabel->"In[13]:=",
