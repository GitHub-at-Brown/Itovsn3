Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"bisector", "=", 
   FractionBox[
    RowBox[{"AA", "+", "XX"}], "2"]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"ZZ", "=", 
   RowBox[{"{", 
    RowBox[{"0", ",", "Z"}], "}"}]}], ";"}], "\n", 
