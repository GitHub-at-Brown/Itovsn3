Cell[CellGroupData[{

Cell["Set up reflection coupling using elementary geometry", "Section",
 Evaluatable->False,
 CellID->1379277448,ExpressionUUID->"8b3aec0b-3ffe-4b05-a526-442c05efe187"],

Cell[CellGroupData[{

Cell["\<\
The next step involves some simple geometry. We construct a unit vector UU \
pointing from (A,B) to (X,Y), and a perpendicular unit vector VV.\
\>", "Text",
 Evaluatable->False,
 CellChangeTimes->{{3.923663628591895*^9, 3.92366362862038*^9}},
 CellID->1577582818,ExpressionUUID->"d8b4396c-d9e9-4be7-9058-fe704c59dda7"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"AA", "=", 
   RowBox[{"{", 
    RowBox[{"A", ",", "B"}], "}"}]}], ";", 
  RowBox[{"XX", "=", 
   RowBox[{"{", 
    RowBox[{"X", ",", "Y"}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"UU", "=", 
   RowBox[{"XX", "-", "AA"}]}], ";"}], "\n", 
 RowBox[{"UU", "=", 
  FractionBox["UU", 
   SqrtBox[
    RowBox[{"UU", ".", "UU"}]]]}], "\n", 
 RowBox[{"VV", "=", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"0", ",", 
       RowBox[{"-", "1"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"1", ",", "0"}], "}"}]}], "}"}], ".", "UU"}]}], "\n", 
 RowBox[{"0", "==", 
  RowBox[{"UU", ".", "VV"}]}]}], "Input",
 PageWidth->DirectedInfinity[1],
 CellLabel->"In[19]:=",
 CellID->1601316264,ExpressionUUID->"564ceee6-7cb2-42c6-ad52-2d402e2ce16e"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   FractionBox[
    RowBox[{
     RowBox[{"-", "A"}], "+", "X"}], 
    SqrtBox[
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "A"}], "+", "X"}], ")"}], "2"], "+", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "B"}], "+", "Y"}], ")"}], "2"]}]]], ",", 
   FractionBox[
    RowBox[{
     RowBox[{"-", "B"}], "+", "Y"}], 
    SqrtBox[
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "A"}], "+", "X"}], ")"}], "2"], "+", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "B"}], "+", "Y"}], ")"}], "2"]}]]]}], "}"}]], "Output",
