Cell[BoxData[
 RowBox[{"-", 
  FractionBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "dH"}], "+", "dL"}], ")"}], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "A"}], "+", "X"}], ")"}], " ", 
    RowBox[{"(", 
     RowBox[{"B", "-", "Y"}], ")"}]}], 
   RowBox[{"2", " ", 
    RowBox[{"(", 
     RowBox[{"A", "+", "X"}], ")"}], " ", 
    SqrtBox[
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"A", "-", "X"}], ")"}], "2"], "+", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"B", "-", "Y"}], ")"}], "2"]}]]}]]}]], "Output",
