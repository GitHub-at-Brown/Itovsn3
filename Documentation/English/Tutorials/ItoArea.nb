}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Print", "[", 
  RowBox[{"\"\<criterion2 = \>\"", ",", 
   RowBox[{"criterion2", "=", 
    RowBox[{"Together", "[", 
     FractionBox[
      RowBox[{"Expand", "[", 
       RowBox[{
        FractionBox["criterion", "dt"], "-", "criterion1"}], "]"}], 
