 CellID->2003553816,ExpressionUUID->"a5f69c2a-baf5-4c4f-9a7c-db1cdc9ac3d4"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(", 
   RowBox[{"driftdV", "=", 
    RowBox[{"Simplify", "[", 
     RowBox[{"Drift", "[", 
      RowBox[{"ItoD", "[", "EVtrial", "]"}], "]"}], "]"}]}], ")"}], "==", 
  "0"}]], "Input",
