Cell[CellGroupData[{

Cell["Compute the answer", "Section",
 Evaluatable->False,
 CellChangeTimes->{3.923675458521223*^9},
 CellID->1458593059,ExpressionUUID->"3aee0e62-0f10-4f14-a2a6-a9180c65172d"],

Cell[CellGroupData[{

Cell["\<\
The answer can now be computed: notice that Mathematica worries here about \
convergence of definite integrals!\
\>", "Text",
 Evaluatable->False,
 CellID->1047712108,ExpressionUUID->"c0106710-4a1e-41d9-a708-6a0b06591073"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"EVtrial", " ", "=", " ", 
  RowBox[{"Refine", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"Exp", "[", "V", "]"}], "/.", "betatrial"}], "/.", 
     "alfatrial"}], ",", 
    RowBox[{
     RowBox[{"t", ">", "t0"}], " ", "&&", " ", 
     RowBox[{"t0", ">", "0"}], " ", "&&", " ", 
     RowBox[{"u", ">", "0"}]}]}], "]"}]}]], "Input",
 PageWidth->DirectedInfinity[1],
 CellLabel->"In[18]:=",
 CellID->1678977403,ExpressionUUID->"3ab70d3f-6b79-4f1f-a2bd-85f09d0c18a5"],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{
    RowBox[{"\[ImaginaryI]", " ", "A", " ", "u"}], "+", 
    RowBox[{
     FractionBox["1", "2"], " ", "u", " ", 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["X", "2"], "+", 
       SuperscriptBox["Y", "2"]}], ")"}], " ", 
     RowBox[{"Tanh", "[", 
      RowBox[{
       RowBox[{"t", " ", "u"}], "-", 
       RowBox[{"t0", " ", "u"}]}], "]"}]}]}]], " ", 
  RowBox[{"Sech", "[", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "t"}], "+", "t0"}], ")"}], " ", "u"}], "]"}]}]], "Output",
 CellChangeTimes->{3.923664682834861*^9, 3.9236731624531116`*^9},
 CellLabel->"Out[18]=",
 CellID->1689790546,ExpressionUUID->"2ea5033e-e8fe-4268-9195-c1496592e0af"]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

