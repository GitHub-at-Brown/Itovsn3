Cell[CellGroupData[{

Cell["\<\
For constant u, consider when the expression exp(V) is a local martingale. \
Here alpha and beta are undetermined deterministic functions vanishing at the \
target time t0. \
\>", "Text",
 Evaluatable->False,
 CellChangeTimes->{{3.923661526551944*^9, 3.923661526592004*^9}, 
   3.923661557891397*^9},
 CellID->1600415495,ExpressionUUID->"3f489d54-1a21-43c0-91de-7f724cfd9f4e"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"SetAttributes", "[", 
   RowBox[{"u", ",", "Constant"}], "]"}], ";"}], "\n", 
 RowBox[{"Print", "[", 
  RowBox[{"\"\<V=\>\"", ",", 
   RowBox[{"V", "=", 
    RowBox[{
     RowBox[{"I", " ", "u", " ", "A"}], "-", 
     RowBox[{
      FractionBox["1", "2"], " ", 
      RowBox[{"\[Alpha]", "[", "t", "]"}], " ", 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["X", "2"], "+", 
        SuperscriptBox["Y", "2"]}], ")"}]}], "+", 
     RowBox[{"\[Beta]", "[", "t", "]"}]}]}]}], "]"}]}], "Input",
 PageWidth->DirectedInfinity[1],
 CellLabel->"In[6]:=",
 CellID->950545790,ExpressionUUID->"39d80c71-b8ba-45d6-aaf9-360c742ab043"],

Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"V=\"\>", "\[InvisibleSpace]", 
   RowBox[{
    RowBox[{"\[ImaginaryI]", " ", "A", " ", "u"}], "-", 
    RowBox[{
     FractionBox["1", "2"], " ", 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["X", "2"], "+", 
       SuperscriptBox["Y", "2"]}], ")"}], " ", 
     RowBox[{"\[Alpha]", "[", "t", "]"}]}], "+", 
    RowBox[{"\[Beta]", "[", "t", "]"}]}]}],
  SequenceForm[
  "V=", Complex[0, 1] $CellContext`A $CellContext`u + 
   Rational[-1, 
      2] ($CellContext`X^2 + $CellContext`Y^2) \
$CellContext`\[Alpha][$CellContext`t] + $CellContext`\[Beta][$CellContext`t]],
  
  Editable->False]], "Print",
 CellChangeTimes->{3.9236646610926337`*^9, 3.923673140767565*^9},
