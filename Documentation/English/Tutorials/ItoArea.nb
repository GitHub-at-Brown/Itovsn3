Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"alfatrial", "=", 
  RowBox[{"First", "[", 
   RowBox[{"DSolve", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"criterion2", "==", "0"}], ",", 
       RowBox[{
        RowBox[{"\[Alpha]", "[", "t0", "]"}], "==", "0"}]}], "}"}], ",", 
     "\[Alpha]", ",", "t"}], "]"}], "]"}]}]], "Input",
