  RowBox[{"{", 
   RowBox[{"\[Alpha]", "\[Rule]", 
    RowBox[{"Function", "[", 
     RowBox[{"t", ",", 
      RowBox[{
       RowBox[{"-", "u"}], " ", 
       RowBox[{"Tanh", "[", 
        RowBox[{"u", " ", 
         RowBox[{"(", 
          RowBox[{"t", "-", "t0"}], ")"}]}], "]"}]}]}], "]"}]}], 
   "}"}]}]], "Input",
