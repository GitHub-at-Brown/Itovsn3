Cell[BoxData[
    \(\(ItoD[f_] := 
      Block[{ff = f /. ItoIntegral[sdx_] \[Rule] ItoIntegral[sdx, t]}, 
        ItoExpand[
          Dt[ff, t]\ dt + 1\/2\ Dt[ff, {t, 2}]\ dt\^2 /. 
            Freeze[sd_] \[Rule] sd]]; \)\)], "Output",
