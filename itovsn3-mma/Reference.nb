Cell[BoxData[
    \(\(BrownSingle[X_, X0_] := 
      Block[{dX = ToExpression["d"<>ToString[X]]}, Introduce[X, dX]; 
        AddQuadVar[dX\^2, dt]; AddDrift[dX, 0]; Fixed[0, X] = X0]; \)\)], 
  "Output",
  Editable->False,
  PageWidth->Infinity,
  Evaluatable->False,
  FontFamily->"Courier New",
  FontSize->12,
